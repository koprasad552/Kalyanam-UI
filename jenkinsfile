pipeline {
    agent {
        docker {
            image 'node:6-alpine'
            args '-p 3000:3000'
        }
    stages {
        stage('gitcheckout'){
            steps {
              git 'https://github.com/koprasad552/Kalyanam-UI.git'
              }
        }      
        stage('Build') {
            steps {
                sh 'npm install'
            }
        }
      }  
   
         
